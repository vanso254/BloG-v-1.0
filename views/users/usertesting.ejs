<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="container">
        <table class="table-hover">
            <thead>
             <tr>
                 <th>id</th>
                 <th>Name</th>
                 <th>Email</th>
                 <th>Gender</th>
                 <th>Role</th>
                 <th>Password</th>
             </tr>
            </thead>
            <tbody>
             <% users.forEach(user=>{ %>
         <tr>
             <td><%= user.id %> </td>
             <td><%= user.name %> </td>
             <td><%= user.email %> </td>
             <td><%= user.gender %> </td>
             <td><%= user.role %> </td>
             <td><%= user.password %> </td>
         </tr>
         <% }) %>
            </tbody>
         </table>
    </div>
</body>
</html>